{
  "$schema": "https://mintlify.com/schema.json",
  "name": "bleed",
  "logo": {
    "dark": "https://bleed.bot/bleed.png",
    "light": "https://bleed.bot/bleed.png"
  },
  "favicon": "favicon.png",
  "colors": {
    "primary": "#658a95",
    "light": "#95b6bd",
    "dark": "#658a95",
    "anchors": {
      "from": "#658a95",
      "to": "#95b6bd"
    }
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "metadata": {
    "theme-color": "#658a95",
    "og:site_name": "bleed.bot"
  },
  "topbarLinks": [
    {
      "name": "Support Server",
      "url": "https://discord.gg/bleed"
    }
  ],
  "topbarCtaButton": {
    "name": "Home",
    "url": "https://bleed.bot"
  },
  "redirects": [
    {
      "source": "/commands",
      "destination": "https://bleed.bot/commands"
    },
    {
      "source": "/status",
      "destination": "https://bleed.bot/status"
    },
    {
      "source": "/discord",
      "destination": "https://discord.gg/bleed"
    }
  ],
  "navigation": [
    {
      "group": "Overview",
      "pages": [
        "overview/introduction",
        "overview/donator-perks",
        "overview/recent-updates"
      ]
    },
    {
      "group": "Security Setup",
      "pages": ["security/moderation", "security/fake-permissions"]
    },
    {
      "group": "Server Configuration",
      "pages": [
        "configuration/system-messages",
        "configuration/level-rewards",
        "configuration/voicemaster"
      ]
    },
    {
      "group": "Integrations",
      "pages": [
        "integrations/spotify",
        "integrations/lastfm",
        "integrations/fortnite",
        "integrations/social-notifications"
      ]
    },
    {
      "group": "Resources",
      "pages": [
        "resources/syntax",
        "resources/variables",
        "resources/permissions",
        "resources/voices",
        "resources/languages"
      ]
    },
    {
      "group": "Common Issues",
      "pages": ["common-issues/ios-dash"]
    }
  ],
  "footerSocials": {
    "discord": "https://discord.gg/bleed",
    "github": "https://github.com/ju/docs"
  }
}
